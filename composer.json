{
    "name": "rocketeers/rocketeer-opcache",
    "description": "Clears Opcache after swapping symlink",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Fabre",
            "email": "ehtnam6@gmail.com"
        }
    ],
    "require": {
        "anahkiasen/rocketeer": "3.0.x@dev",
        "gordalina/cachetool": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Rocketeer\\Plugins\\Opcache\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
